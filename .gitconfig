[alias]
	st = status
	br = branch
	ch = checkout
	ci = commit
	tree = log --graph --decorate --pretty=oneline --abbrev-commit
	unmerge = reset --merge ORIG_HEAD
	new = !sh -c 'git log $1@{1}..$1@{0} "$@"'
